<%
	CommentsDAO dao = 	new CommentsDAO();
	String resultMsg = null;

	if(dao.insert(comments)==0){
		resultMsg = "코멘트 등록실패";
	}else{
		resultMsg = "코멘트 등록성공";
	}
%>
